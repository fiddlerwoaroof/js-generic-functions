{
  "compilerOptions": {
    "target": "es2020",
    "baseUrl": "src",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "checkJs": true,
    "lib": ["es2020"],
    "rootDirs": ["src"],
    "module": "commonjs"
  },
  "exclude": [
    ".history/*",
    "frontend",
    "node_modules",
    "node_modules/**/*",
    "**/node_modules/**/*",
    "**/node_modules/*",
    "/.#*",
    "**/.#*"
  ]
}
